  #   document.set('author.title' => 'Sir')
  #   document.set('author.name' => 'Linus Torvalds')
  #   # => document.author == {'title' => 'Sir', 'name' => 'Linus Torvalds'}
  # @example Nested hash overwriting a non-hash value.
  #   document.set('author' => 'John Doe')
